id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="WishlistMovies" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="31"/><column index="2" value="34"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sqlite_sequence" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="44"/><column index="2" value="32"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE User (

    uID TEXT PRIMARY KEY,

    Password TEXT NOT NULL,

    gender TEXT CHECK(gender IN ('남성', '여성')) NOT NULL,

    age INTEGER CHECK(age &gt; 0),

    email TEXT UNIQUE NOT NULL

);



-- Movie 테이블

CREATE TABLE Movie (

    mID TEXT PRIMARY KEY,
	
	mName TEXT,
	
	Genre TEXT

);



-- WatchedMovies 테이블 (감상한 영화)

CREATE TABLE WatchedMovies (

    uID TEXT NOT NULL,

    mID TEXT NOT NULL,

    PRIMARY KEY (uID, mID),

    FOREIGN KEY (uID) REFERENCES User(uID) ON DELETE CASCADE,

    FOREIGN KEY (mID) REFERENCES Movie(mID) ON DELETE CASCADE

);



-- WishlistMovies 테이블 (보고 싶은 영화)

CREATE TABLE WishlistMovies (

    uID TEXT NOT NULL,

    mID TEXT NOT NULL,

    PRIMARY KEY (uID, mID),

    FOREIGN KEY (uID) REFERENCES User(uID) ON DELETE CASCADE,

    FOREIGN KEY (mID) REFERENCES Movie(mID) ON DELETE CASCADE

);



CREATE TABLE FriendList (

    uID TEXT NOT NULL,           

    friendID TEXT NOT NULL,      

    PRIMARY KEY (uID, friendID), 

    FOREIGN KEY (uID) REFERENCES User(uID ) ON DELETE CASCADE, 

    FOREIGN KEY (friendID) REFERENCES User(uID) ON DELETE CASCADE

);

CREATE TABLE UserGenreScore (
		
	---랜덤뽑기용 번호
	u_number INTEGER PRIMARY KEY AUTOINCREMENT,
	
	---유저이름
	username TEXT,

	---드라마
	drama INTEGER,

	---멜로/로맨스
	meloRomance INTEGER,

	---액션
	action INTEGER,

	---코미디
	comedy INTEGER,

	---스릴러
	thriller INTEGER,

	---성인물(에로)
	ero INTEGER,

	---공포
	horror INTEGER,

	---범죄
	crime INTEGER,

	---애니메이션
	animation INTEGER,

	---어드벤처
	adventure INTEGER,

	---SF
	sf INTEGER,

	---판타지
	fantasy INTEGER,

	---미스터리
	mystery INTEGER,

	---다큐멘터리
	documentary INTEGER,

	---가족
	family INTEGER,

	---사극
	historical INTEGER,

	---전쟁
	war INTEGER,

	---공연
	performing INTEGER,

	---뮤지컬
	musical INTEGER,

	---서부극
	western INTEGER,

	---기타
	other INTEGER,

	FOREIGN KEY (username) REFERENCES User(uID) ON DELETE CASCADE
);

CREATE TABLE COMMUNITY (
	cID INTEGER PRIMARY KEY AUTOINCREMENT,

	---제목
	title TEXT,

	---내용
	content TEXT,

	---영화제목
	moviename TEXT,

	---작성자
	writer TEXT,
	
	FOREIGN KEY (moviename) REFERENCES Movie(mName),
	
	FOREIGN KEY (writer) REFERENCES User(uID)
);

